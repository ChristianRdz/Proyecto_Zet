{% extends "layout.html.twig" %}

{% block view %}
<h1 class="page-header">Usuarios</h1>
<table class="table">
  <thead>
    <th>Nombre</th>
    <th>Apellidos</th>
    <th>Edad</th>
    <th>E-mail</th>
    <th>Ocupacion</th>
  </thead>
  <tbody>
    {% for usuario in usuarios %}
    <tr>
       <td>{{usuario.nombre}}</td>
       <td>{{usuario.apellidos}}</td>
       <td>{{usuario.edad}}</td>
       <td>{{usuario.email}}</td>
       <td>{{usuario.ocupacion}}</td>
       
    </tr>
      {% endfor %}
  </tbody>
</table>

{% endblock %}
