<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
  <title>{% block title %}proyecto_zet{% endblock %}</title>
  {% block stylesheets %}
  <link rel="stylesheet"type="text/css" href="{{asset('css/bootstrap.min.css') }}">
<link href="{{ asset('css/style2.css') }}" type="text/css" rel="stylesheet" />
<link href="{{ asset('css/bootstrap.css') }}" type="text/css" rel="stylesheet" />
<link href="{{ asset('css/font-awesome.min.css') }}" type="text/css" rel="stylesheet" />

<!--<link href="{{ asset('css/bootstrap2.min.css') }}" type="text/css" rel="stylesheet" /> -->

    {% endblock %}

    <link rel="shortcut icon" type=imagen/x-icon href="img/logotipo.png">
  <body>
<!--inicio de nevegador bootstrap-->
<nav class="navbar navbar-inverse">
<div class="container-fluid">
  <div class="navbar-header">
    <a class="navbar-brand" href="{{path('zet_homepage')}}">ZET KINGDOM</a>
  </div>
  <ul class="nav navbar-nav">
    <li ><a href="{{path('zet_homepage')}}">Home</a></li>
    <li><a href="{{path('rutazet')}}">Zelda</a></li>
    <li><a href="{{path('otrozet')}}">Fire Emblem</a></li>
    <li><a href="{{path('staruta')}}">Earth Bound</a></li>
  </ul>
  <ul class="nav navbar-nav navbar-right">
    <li><a href="{{path('rutaUsuarios')}}"><span class="glyphicon glyphicon-user"></span> Suscribirse</a></li>
    <li><a href="{{path('rutagamer')}}"><span class="glyphicon glyphicon-log-in"></span> Miembros</a></li>
  </ul>
</div>
</nav>
<!--final del navegador bootstrap-->

{% block view %}
<h1 class="page-header">Usuarios</h1>
<table class="table">
  <thead>
    <th>Nombre</th>
    <th>Apellidos</th>
    <th>Edad</th>
    <th>E-mail</th>
    <th>Ocupacion</th>
  </thead>
  <tbody>
    {% for usuario in usuarios %}
    <tr>
       <td>{{usuario.nombre}}</td>
       <td>{{usuario.apellidos}}</td>
       <td>{{usuario.edad}}</td>
       <td>{{usuario.email}}</td>
       <td>{{usuario.ocupacion}}</td>

    </tr>
      {% endfor %}
  </tbody>
</table>

{% endblock %}


  <script src="{{ asset('js/jquery-3.2.1.min.js') }}" charset="utf-8"></script>
  <script src="{{ asset('js/bootstrap.min.js') }}" charset="utf-8"> </script>
  <script src="{{ asset('js/bootstrap.js') }}" charset="utf-8"> </script>
    </body>
  </html>
